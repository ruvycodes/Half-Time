{
  "manifest_version": 3,
  "name": "Playback Speed Timer",
  "version": "1.0",
  "description": "Calculate time saved by changing YouTube video playback speed.",
  "permissions": ["activeTab" , "tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
